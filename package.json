{
  "name": "geohash",
  "description": "A fork of Chris Veness', Gustavo Niemeyerâ€™s geocoding system implementation adding typescript.",
  "homepage": "https://github.com/gabrielmedici/geohash",
  "author": "Chris Veness",
  "contributors": [
    {
      "name": "Gabriel Medici"
    }
  ],
  "version": "2.0.1",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.ts",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielmedici/geohash"
  },
  "keywords": [
    "geohash",
    "geocode",
    "geolocation",
    "latitude",
    "longitude"
  ],
  "scripts": {
    "build": "tsc",
    "test": "mocha --exit -r esm test.js",
    "lint": "eslint index.ts"
  },
  "devDependencies": {
    "chai": "^4.0.0",
    "eslint": "^6.0.0",
    "esm": "^3.0.0",
    "mocha": "^6.0.0",
    "typescript": "^5.1.6"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "mocha": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "rules": {
      "array-bracket-spacing": [
        "error",
        "always"
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "comma-spacing": [
        "error"
      ],
      "curly": [
        "error",
        "multi-line"
      ],
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1
        }
      ],
      "key-spacing": [
        "error",
        {
          "align": "value"
        }
      ],
      "keyword-spacing": [
        "error"
      ],
      "no-case-declarations": "warn",
      "no-console": [
        "warn",
        {
          "allow": [
            "error",
            "info"
          ]
        }
      ],
      "no-irregular-whitespace": "warn",
      "no-redeclare": "warn",
      "no-shadow": "warn",
      "no-unused-vars": "warn",
      "no-var": "error",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "prefer-const": "error",
      "quotes": [
        "error",
        "single",
        "avoid-escape"
      ],
      "semi": [
        "error",
        "always"
      ],
      "space-before-blocks": [
        "error",
        "always"
      ],
      "space-in-parens": [
        "error"
      ],
      "strict": [
        "error",
        "global"
      ]
    }
  }
}
